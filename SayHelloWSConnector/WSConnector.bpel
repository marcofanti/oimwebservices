          <reply name="DeleteOpReply" variable="DeleteOpReply_OutputVariable"
                 partnerLink="wsconnector_client"
                 portType="ns1:WebservicesConnectorService" operation="delete"/>
        </sequence>
      </onMessage>
      <onMessage variable="UpdateOp_InputVariable"
